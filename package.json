{
  "name": "arm-templator",
  "version": "0.0.24",
  "description": "ARM Typescript template generator prototype",
  "main": "dist/index",
  "bin": {
    "arm-deploy": "dist/cli/deploy.js",
    "arm-display": "dist/cli/display.js"
  },
  "typings": "dist/index",
  "scripts": {
    "schema": "ts-node cli/gen-schema",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "watch": "tsc -w -p .",
    "clean": "rimraf ./dist",
    "test": "mocha -r ts-node/register tests/run.ts"
  },
  "bugs": {
    "url": "https://github.com/anthony-c-martin/arm-templator/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/anthony-c-martin/arm-templator.git"
  },
  "keywords": [
    "azure",
    "arm",
    "arm template",
    "azure resource manager"
  ],
  "author": "Anthony Martin <anthony.ct.martin@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "async": "^3.2.0",
    "azure-arm-resource": "^7.3.0",
    "chalk": "^3.0.0",
    "ms-rest-azure": "^3.0.0",
    "yargs": "^15.2.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.8.3"
  },
  "devDependencies": {
    "@types/async": "^3.0.8",
    "@types/chai": "^4.2.9",
    "@types/mocha": "^7.0.1",
    "@types/node": "^13.7.7",
    "@types/yargs": "^15.0.4",
    "chai": "^4.2.0",
    "mocha": "^7.1.0",
    "rimraf": "^3.0.2",
    "tsc": "^1.20150623.0"
  },
  "files": [
    "dist"
  ]
}

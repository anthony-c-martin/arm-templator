{
  "name": "arm-templator",
  "version": "0.0.21",
  "description": "ARM Typescript template generator prototype",
  "main": "dist/template",
  "bin": {
    "arm-deploy": "dist/cli/deploy.js",
    "arm-display": "dist/cli/display.js"
  },
  "typings": "dist/template",
  "scripts": {
    "deploy": "ts-node lib/cli/deploy.ts",
    "display": "ts-node lib/cli/display.ts",
    "schema": "ts-node cli/gen-schema",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "watch": "tsc -w -p .",
    "clean": "rimraf ./dist",
    "test": "mocha -r ts-node/register tests/run.ts"
  },
  "bugs": {
    "url": "https://github.com/anthony-c-martin/arm-templator/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/anthony-c-martin/arm-templator.git"
  },
  "keywords": [
    "azure",
    "arm",
    "arm template",
    "azure resource manager"
  ],
  "author": "Anthony Martin <anthony.ct.martin@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "async": "^3.1.0",
    "azure-arm-resource": "^7.3.0",
    "chalk": "^3.0.0",
    "ms-rest-azure": "^3.0.0",
    "yargs": "^15.1.0",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@types/async": "^3.0.3",
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.8",
    "@types/yargs": "^15.0.3",
    "chai": "^4.2.0",
    "mocha": "^7.0.0",
    "rimraf": "^3.0.1",
    "tsc": "^1.20150623.0"
  },
  "files": [
    "dist"
  ]
}

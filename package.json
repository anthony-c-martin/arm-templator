{
  "name": "arm-templator",
  "version": "0.0.5",
  "description": "ARM Typescript template generator prototype",
  "main": "dist/template",
  "typings": "dist/template",
  "scripts": {
    "start": "ts-node cli/gen-template",
    "schema": "ts-node cli/gen-schema",
    "prepublishOnly": "npm run compile",
    "compile": "npm run clean && tsc -p .",
    "watch": "tsc -w -p .",
    "clean": "rm -rf dist",
    "test": "mocha -r ts-node/register tests.ts"
  },
  "bugs": {
    "url": "https://github.com/anthony-c-martin/arm-templator/issues"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/anthony-c-martin/arm-templator.git"
  },
  "keywords": [
    "azure",
    "arm",
    "arm template",
    "azure resource manager"
  ],
  "author": "Anthony Martin <anthony.ct.martin@gmail.com>",
  "license": "ISC",
  "dependencies": {},
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/mocha": "^5.2.7",
    "@types/node": "^13.1.8",
    "chai": "^4.2.0",
    "mocha": "^7.0.0",
    "ts-node": "^8.6.2",
    "tsc": "^1.20150623.0",
    "typescript": "^3.7.5"
  },
  "files": [
    "dist"
  ]
}
